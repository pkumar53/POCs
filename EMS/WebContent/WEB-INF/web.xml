    <servlet-class>com.org.ems.controller.EmployeeListController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeListController</servlet-name>
    <url-pattern>/EmployeeListController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>com.org.ems.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AuthController</servlet-name>
    <servlet-class>com.org.ems.controller.AuthController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AuthController</servlet-name>
    <url-pattern>/AuthController</url-pattern>
  </servlet-mapping>
